{
  "name": "reactjs.client.web",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "build": "node scripts/build.js",
    "review": "node scripts/review.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^constants$": "constants",
      "^views$": "views",
      "^utils$": "utils",
      "^styles$": "styles",
      "^services$": "services",
      "^models$": "models",
      "^components$": "components"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@mdi/font": "^5.3.45",
    "@microsoft/signalr": "^3.1.5",
    "@rjsf/core": "^2.2.1",
    "@rjsf/material-ui": "^2.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^7.0.0-0",
    "babel-preset-react-app": "^9.1.2",
    "babel-runtime": "^7.0.0-0",
    "bootstrap": "^4.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "connected-react-router": "^6.8.0",
    "crypto-js": "^4.0.0",
    "css-loader": "^3.5.3",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "i18n": "^0.10.0",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "immutable": "^3.8.1",
    "install": "^0.13.0",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.5.3",
    "npm": "^6.14.5",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "2.0.6",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-datetime": "^2.15.0",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.5.0",
    "react-jsonschema-form": "^1.8.1",
    "react-recaptcha-v3": "^2.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-select": "^3.0.8",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.1",
    "redux-dynamic-middlewares": "^0.4.0",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^1.2.1",
    "url-loader": "^1.1.1",
    "url-search-params-polyfill": "^2.0.1",
    "urlencode": "^1.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.20.0"
  }
}
